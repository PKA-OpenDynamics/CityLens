# =============================================================================
# Data Source Metadata
# =============================================================================

cl-data:OpenWeatherMapSource a prov:Agent, dcat:DataService ;
    rdfs:label "OpenWeatherMap API"@en ;
    dct:description "Real-time weather data from OpenWeatherMap"@en ;
    dcat:endpointURL <https://api.openweathermap.org/data/2.5/> .

cl-data:AQICNSource a prov:Agent, dcat:DataService ;
    rdfs:label "AQICN API"@en ;
    dct:description "World Air Quality Index project data"@en ;
    dcat:endpointURL <https://aqicn.org/api/> .

cl-data:TomTomSource a prov:Agent, dcat:DataService ;
    rdfs:label "TomTom Traffic API"@en ;
    dct:description "Real-time traffic flow data from TomTom"@en ;
    dcat:endpointURL <https://api.tomtom.com/traffic/> .
